<%- model_class = Proyect -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.proyecto', 'Proyecto'], :model=> model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @proyect.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @proyect.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:begindate) %>:</strong></dt>
  <dd><%= @proyect.begindate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:enddate) %>:</strong></dt>
  <dd><%= @proyect.enddate %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.Atras")),
              proyects_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.editar")),
              edit_proyect_engineer(@proyect,engineer), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.Eliminar")),
              proyect_engineer_path(@proyect,engineer),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmar", :default => 'Estas seguro?')) },
              :class => 'btn btn-danger' %>
</div>
